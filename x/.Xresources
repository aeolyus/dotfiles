!base16 tomorrow color scheme
*background: #1d1f21
*foreground: #c5c8c6
! black dark/light
*color0: #1d1f21
*color8: #969896
! red dark/light
*color1: #cc6666
*color9: #cc6666
! green dark/light
*color2: #b5bd68
*color10: #b5bd68
! yellow dark/light
*color3: #f0c674
*color11: #f0c674
! blue dark/light
*color4: #81a2be
*color12: #81a2be
! magenta dark/light
*color5: #b294bb
*color13: #b294bb
! cyan dark/light
*color6: #8abeb7
*color14: #8abeb7
! white dark/light
*color7: #c5c8c6
*color15: #ffffff

! URxvt General Settings
URxvt.scrollBar: False
URxvt.cursorBlink: True
URxvt.termName: rxvt-unicode-256color
URxvt.depth: 32
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.saveLines: 1000
URxvt.internalBorder: 5
! Shut up rxvt
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.print-pipe: "cat > /dev/null"
! URxvt Font
URxvt.font: xft:Hack Nerd Font Mono:size=11
!URxvt.letterSpace: -1
! URxvt Extensions
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.resize-font.bigger: C-equal
URxvt.resize-font.smaller: C-minus
URxvt.resize-font.reset: C-0

! General X Settings
Xcursor.size: 16
Xft.dpi: 96
Xft.rgba: rgb
Xft.hinting: 1
Xft.autohint: 0
Xft.antialias: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
