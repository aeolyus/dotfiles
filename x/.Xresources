! URxvt General Settings
URxvt.scrollBar: False
URxvt.cursorBlink: True
URxvt.termName: rxvt-unicode-256color
URxvt.depth: 32
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.saveLines: 1000
URxvt.internalBorder: 10
! Shut up rxvt
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.print-pipe: "cat > /dev/null"
! URxvt Font
URxvt.font: xft:Hack Nerd Font Mono:size=11
! URxvt.letterSpace: -1
! URxvt Extensions
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select,selection-to-clipboard
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.resize-font.bigger: C-equal
URxvt.resize-font.smaller: C-minus
URxvt.resize-font.reset: C-0

! General X Settings
Xcursor.size: 16
Xft.dpi: 96
Xft.rgba: rgb
Xft.hinting: 1
Xft.autohint: 0
Xft.antialias: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
